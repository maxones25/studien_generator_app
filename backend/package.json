{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:admin": "nest start --entryFile admin",
    "start:admin:dev": "nest start --entryFile admin --watch",
    "start:admin:debug": "nest start --entryFile admin --debug --watch",
    "start:admin:prod": "node dist/src/admin",
    "start:study": "nest start --entryFile study",
    "start:study:dev": "nest start --entryFile study --watch",
    "start:study:debug": "nest start --entryFile study --debug --watch",
    "start:study:prod": "node dist/src/study",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:admin:unit": "jest --config admin-unit.jest.config.ts",
    "test:admin:unit:dev": "jest --config admin-unit.jest.config.ts --onlyChanged --watch",
    "test:admin:integration": "jest --config admin-integration.jest.config.ts",
    "test:admin:integration:dev": "jest --config admin-integration.jest.config.ts --onlyChanged",
    "db:migration:run": "typeorm-ts-node-commonjs migration:run -d data-source.ts",
    "db:migration:revert": "typeorm-ts-node-commonjs migration:revert -d data-source.ts",
    "db:migration:create": "typeorm-ts-node-commonjs migration:create",
    "db:sync": "typeorm-ts-node-commonjs schema:sync -- -d data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-and-time": "^3.0.2",
    "generate-password": "^1.7.0",
    "mysql2": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@automock/jest": "^1.2.2",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "madge": "^6.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
